{
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "helmet": "^3.12.1",
    "humps": "^2.0.1",
    "immutable": "^3.8.2",
    "next": "^8.1.0",
    "next-redux-wrapper": "^2.1.0",
    "next-routes": "^1.4.2",
    "prop-types": "^15.6.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0",
    "styled-reset": "^2.0.13"
  },
  "name": "nextjs-redux-starter",
  "version": "1.0.0",
  "main": "server/index.js",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@testing-library/react": "^8.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "jest": "^24.7.1",
    "jest-dom": "^3.4.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.12.1",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "build": "NODE_ENV=production next build",
    "test:ci": "jest --maxWorkers=2 --ci",
    "test": "jest --watch",
    "start": "nodemon -w server server/index.js",
    "serve": "NODE_ENV=production node server/index.js",
    "clean": "rimraf node_modules/.cache .next",
    "lint": "standard --verbose | snazzy",
    "now-start": "yarn serve",
    "lint:fix": "standard --fix --verbose | snazzy"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "engines": {
    "node": ">= 8"
  },
  "jest": {
    "modulePaths": [
      "./src",
      "./src/test"
    ],
    "setupFilesAfterEnv": [
      "jest-dom/extend-expect",
      "@testing-library/react/cleanup-after-each"
    ]
  },
  "repository": "CodementorIO/nextjs-redux-starter",
  "bugs": "https://github.com/CodementorIO/nextjs-redux-starter/issues",
  "author": "Ian Wang <gogoenwon@gmail.com> (https://github.com/IanWang)",
  "license": "ISC",
  "description": "Opinionated Next.js starter with Express, Redux, SASS, and Jest."
}
